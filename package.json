{
  "name": "myproject",
  "version": "1.0.0",
  "description": "express project",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node ./src/bin/www.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/config/db.ts",
    "local": "npx nodemon -r tsconfig-paths/register --ignore node_modules/ --ignore test/ src/bin/www.ts",
    "local:force": "ts-node ./node_modules/typeorm/cli.js --dataSource src/config/db.ts",
    "migration:generate": "NODE_ENV=local npx typeorm-ts-node-commonjs migration:generate -d ./src/config/db.ts -p -o ./src/database/migrations/",
    "migration:run": "NODE_ENV=local npx typeorm-ts-node-commonjs migration:run -d ./src/config/db.ts"
  },
  "author": "",
  "license": "ISC",
