{
  "name": "myproject",
  "version": "1.0.0",
  "description": "express project",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node app.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/config/db.ts",
    "local": "ts-node app.ts --env local",
    "local:force": "ts-node ./node_modules/typeorm/cli.js --dataSource src/config/db.ts",
    "migration:generate": "NODE_ENV=local npx typeorm-ts-node-commonjs migration:generate -d ./src/config/db.ts -p -o ./src/database/migrations/",
    "migration:run": "NODE_ENV=local npx typeorm-ts-node-commonjs migration:run -d ./src/config/db.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@js-joda/core": "^5.6.1",
    "@js-joda/timezone": "^2.18.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/passport": "^1.0.15",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "http-status": "^1.7.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.1",
    "passport": "^0.6.0",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^5.0.0",
    "ts-jenum": "^2.2.2",
    "typeorm": "^0.3.17",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.3",
    "@types/swagger-jsdoc": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.5",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "swagger-jsdoc": "^6.2.8",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
